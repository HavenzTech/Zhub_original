# HavenzHub Frontend Environment Variables
# Copy this file to .env.local and update with your values

# ASP.NET BMS API Configuration (Backend API)
# This is your main backend API that handles auth, documents, folders, etc.
NEXT_PUBLIC_BMS_API_BASE_URL=http://localhost:5087

# Auth API URL (usually same as BMS API)
NEXT_PUBLIC_API_URL=http://localhost:5087

# Python AI Backend (RAG, Document Analysis)
NEXT_PUBLIC_API_BASE_URL=http://localhost:8001

# API Request Timeout (in milliseconds)
NEXT_PUBLIC_API_TIMEOUT=30000
